            <div id="layoutSidenav_content">
                <main>
                    <div class="container-fluid px-4">
                        <h1 class="mt-4">Painel de Controle</h1>
                        <ol class="breadcrumb mb-4">
                            <li class="breadcrumb-item active">Tabela 'users'</li>
                        </ol>
                        </div>
                            <div class="card mb-4">
                                <div class="card-header">
                                    <i class="fas fa-table me-1"></i>
                                    Informações referentes aos usuários
                                </div>
                                <div class="card-body">
                                    <table class="excel-tableB" id="datatablesSimple">
                                        <thead>
                                            <tr>
                                                <th name="A" >Usuário</th>
                                                <th name="A" >Senha</th>
                                                <th name="A" >CPF</th>
                                                <th name="B" >Sexo</th>
                                                <th name="B" >datanascimento</th>
                                                <th name="A" >Email</th>
                                                <th name="B" >Tipo</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                          <ul>
                                            <% users.forEach(users => { %>
                                              <table class="excel-tableB">
                                                <td><%=users.username%></td><td><%=users.password%></td><td><%=users.cpf%></td>
                                                <td><%=users.sexo%></td><td><%=users.datanascimento%></td> <td><%=users.email%></td>
                                                <td><%=users.tipo%></td>  
                                              </table>
                                            <% }); %>
                                          </ul>
                                        </tbody>
                                    </table>
                                </div>
                        </div>
                        <div class="card mb-4">
                            <div class="card-header">
                                <i class="fas fa-table me-1"></i>
                                Informações referentes a agendamentos
                            </div>
                            <div class="card-body">
                               <iframe width="1750px" height="1000px" class="consultas" src="medicopage"></iframe>
                            </div>
                    </div>
                    <div class="card mb-4">
                        <div class="card-header">
                            <i class="fas fa-table me-1"></i>
                            Informações referentes a mensagens feitas
                        </div>
                        <div class="card-body">
                           <iframe width="1750px" height="1000px" class="consultas" src="gestorpage"></iframe>
                        </div>
                </div>
